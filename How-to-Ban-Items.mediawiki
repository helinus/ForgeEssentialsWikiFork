
####################
# nocraft
#===================
# Configuration options to remove an item's crafting recipe.
####################

nocraft {
    # Use this format: "id:meta". Use meta -1 to ban ALL variants of an item/block.
    S:List <
        9278
        9279
     >
}


####################
# nouse
#===================
# Configuration options to make an item unusable.
####################

nouse {
    # Use this format: "id:meta". Use meta -1 to ban ALL variants of an item/block.
    S:List <
	    25263
		9278
		9279
     >
}
